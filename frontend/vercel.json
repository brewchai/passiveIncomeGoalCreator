{
    "rewrites": [
        {
            "source": "/about",
            "destination": "/about.html"
        },
        {
            "source": "/privacy",
            "destination": "/privacy.html"
        },
        {
            "source": "/terms",
            "destination": "/terms.html"
        },
        {
            "source": "/contact",
            "destination": "/contact.html"
        },
        {
            "source": "/blog",
            "destination": "/blog.html"
        },
        {
            "source": "/blog/:slug",
            "destination": "/blog/posts/:slug/index.html"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}